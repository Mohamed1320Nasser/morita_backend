{
    "name": "Morita_Backend",
    "version": "1.0.0",
    "description": "",
    "main": "build/app.js",
    "scripts": {
        "build": "tsc --build",
        "dev": "nodemon --exec ts-node src/app.ts",
        "dev:bot": "nodemon --exec ts-node src/discord-bot/start.ts",
        "dev:both": "npm run dev & npm run dev:bot",
        "start": "node build/app.js",
        "start:bot": "node build/discord-bot/start.js",
        "start:both": "npm run start & npm run start:bot",
        "start:pm2": "pm2 start ecosystem.config.js --env production",
        "start:pm2:bot": "pm2 start ecosystem.bot-only.js",
        "stop:pm2": "pm2 stop ecosystem.config.js",
        "restart:pm2": "pm2 restart ecosystem.config.js",
        "restart:pm2:bot": "pm2 restart morita-bot",
        "logs:pm2": "pm2 logs",
        "logs:pm2:bot": "pm2 logs morita-bot",
        "reinstall": "rm -rf node_modules && rm -f package-lock.json && npm cache clean --force && npm install",
        "format": "prettier --write \"src/**/*.{ts,js,json}\"",
        "format:check": "prettier --check \"src/**/*.{ts,js,json}\"",
        "db:generate": "npx prisma generate",
        "db:migrate": "npx prisma migrate dev",
        "db:studio": "npx prisma studio",
        "db:push": "npx prisma db push",
        "db:seed": "npx prisma db seed",
        "seed:industries": "ts-node scripts/seed-industries.ts",
        "seed:gaming": "ts-node src/scripts/seed-gaming-services.ts",
        "docs:generate": "ts-node scripts/simple-docs-generator.ts",
        "docs:serve": "ts-node scripts/simple-docs-generator.ts"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@cspell/dynamic-import": "^9.2.1",
        "@prisma/client": "^6.16.1",
        "@sentry/node": "^10.11.0",
        "@sentry/profiling-node": "^10.11.0",
        "@sentry/tracing": "^7.120.4",
        "adm-zip": "^0.5.16",
        "axios": "^1.12.2",
        "bcryptjs": "^3.0.2",
        "body-parser": "^2.2.0",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.2",
        "class-validator-jsonschema": "^5.1.0",
        "cors": "^2.8.5",
        "discord.js": "^14.15.3",
        "dotenv": "^17.2.3",
        "express": "^5.1.0",
        "express-rate-limit": "^8.1.0",
        "express-useragent": "^1.0.15",
        "fast-glob": "^3.3.3",
        "glob": "^11.0.3",
        "google-auth-library": "^10.3.0",
        "helmet": "^8.1.0",
        "isomorphic-dompurify": "^2.26.0",
        "jsonwebtoken": "^9.0.2",
        "mime-types": "^3.0.1",
        "morgan": "^1.10.1",
        "multer": "^2.0.2",
        "mysql": "^2.18.1",
        "nodemailer": "^7.0.6",
        "openapi-to-postmanv2": "^5.1.0",
        "prisma": "^6.16.1",
        "reflect-metadata": "^0.2.2",
        "routing-controllers": "^0.11.3",
        "routing-controllers-openapi": "^5.0.0",
        "slugify": "^1.6.6",
        "swagger-ui-express": "^5.0.1",
        "typedi": "^0.10.0",
        "uuid": "^9.0.1",
        "winston": "^3.17.0",
        "winston-daily-rotate-file": "^5.0.0",
        "xlsx": "^0.18.5"
    },
    "devDependencies": {
        "@types/adm-zip": "^0.5.7",
        "@types/body-parser": "^1.19.6",
        "@types/cors": "^2.8.19",
        "@types/express": "^5.0.3",
        "@types/express-useragent": "^1.0.5",
        "@types/file-type": "^10.9.3",
        "@types/jsonwebtoken": "^9.0.10",
        "@types/mime-types": "^3.0.1",
        "@types/morgan": "^1.9.10",
        "@types/multer": "^2.0.0",
        "@types/node": "^24.5.1",
        "@types/nodemailer": "^7.0.1",
        "@types/swagger-ui-express": "^4.1.8",
        "@types/uuid": "^9.0.8",
        "debug": "^4.4.1",
        "prettier": "^3.6.2",
        "ts-node": "^10.9.2",
        "typescript": "^5.9.2"
    }
}
